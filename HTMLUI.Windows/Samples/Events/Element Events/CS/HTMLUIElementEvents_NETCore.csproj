<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>HTMLUIElementEvents</AssemblyName>
    <RootNamespace>Syncfusion.Windows.Forms.HTMLUI.Samples</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="events.htm" />
    <None Remove="freetrial.gif" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="events.htm" />
    <EmbeddedResource Include="freetrial.gif">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.HTMLUI.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.HTMLUI.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.MIME.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Edit.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Grid.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Scripting.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Scripting.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="17.4.0.39" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.4.0.39" />
  </ItemGroup>
</Project>