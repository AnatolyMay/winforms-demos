<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>DataMarkers</AssemblyName>
    <RootNamespace>DataMarkers</RootNamespace>
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="mappath.png" />
    <None Remove="pin.png" />
    <None Remove="world1.shp" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="mappath.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="pin.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <EmbeddedResource Include="world1.shp">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Maps.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Calculate.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
</Project>