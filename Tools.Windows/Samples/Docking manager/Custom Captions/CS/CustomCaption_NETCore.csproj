<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>CustomCaption</AssemblyName>
    <RootNamespace>CustomCaption</RootNamespace>
    <DefineConstants>TRACE;DEBUG;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Grid.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Office2019Theme.WinForms" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>
</Project>