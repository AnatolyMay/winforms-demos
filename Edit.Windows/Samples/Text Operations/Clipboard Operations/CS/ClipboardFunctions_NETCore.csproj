<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ClipboardFunctionsDemo</AssemblyName>
    <RootNamespace>ClipboardFunctionsDemo</RootNamespace>
    <DefineConstants>NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Test Files\Test.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="logo.ico" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Test Files\Test.cs">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Edit.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SpellChecker.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Grid.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Windows" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
</Project>